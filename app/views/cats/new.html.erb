<h1>New cat</h1>

<%= form_with model: @cat do |form| %>
  <%= form.text_field :name, placeholder: "name" %>
  <%= form.text_field :color, placeholder: "color" %>
  <%= form.text_field :breed, placeholder: "breed" %>
  <%= form.submit %>
<% end %>

<div>
  <%= link_to "Back to cats", cats_path %>
</div>
